
<div>
  <h4><%= @user.username %></h4>
</div>
<div>
  <h4>
    <%= @user.email %>
  </h4>
</div>
<div>
<h3>Stuff you added</h3>
  <% @user.created_attractions.each do |a| %>
    <div>
     <%= link_to a.name, attraction_path(a) %>
	</div>
  <% end %>
</div>
<div>
<h3>Stuff you've seen</h3>
  <% @user.visited_attractions.each do |a| %>
    <div>
      <%= link_to a.name, attraction_path(a) %>
	</div>
  <% end %>
</div>