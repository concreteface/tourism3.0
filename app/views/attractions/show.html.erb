<div class='show row'>
  <div class='small-12 medium-8 large-6 columns'>
    <img src="<%= @attraction.photo_url %>" height='700' width='700'>
    <div class='row'>
      <strong>Name:</strong>
      <%= @attraction.name %>
    </div>
    <div class='row'>
      <strong>Description:</strong>
      <%= @attraction.description %>
    </div>
    <div class='row'>
      <% if @attraction.creator == current_user %>
        <%= link_to 'Edit Picture Details', edit_attraction_path(@attraction) %>
      <% end %>
    </div>
  </div>
  <div >
    <iframe width="600" height="450" frameborder="0" style="border:0" src='<%= @iframe_source %>' id='map' allowfullscreen></iframe>
  </div>
</div>
