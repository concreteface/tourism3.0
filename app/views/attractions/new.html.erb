<%= form_for @attraction do |f| %>
<div>
<%= f.label :name %>
<%= f.text_field :name %>
</div>
<div>
<%= f.label :description %>
<%= f.text_field :description %>
</div>
<div>
<%= f.label :photo, "Select Photo" %>
<%= f.file_field :photo %>
</div>
<div class='actions'>
<%= f.submit "Submit", class: 'button'  %>
</div>
<% end %>

<%= link_to 'Cancel', :back, class: 'button' %>